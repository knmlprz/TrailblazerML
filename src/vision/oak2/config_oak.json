{
    "rgb_camera": {
        "resolution": "THE_1080_P",
        "socket": "CAM_A",
        "isp_scale": [1, 3],
        "fps": 5
    },
    "mono_cameras": {
        "resolution": "THE_400_P",
        "fps": 5
    },
    "depth": {
        "preset": "HIGH_DENSITY",
        "median_filter": "KERNEL_7x7",
        "left_right_check": false,
        "extended_disparity": false,
        "subpixel": true,
        "align_to": "CAM_A",
        "threshold_filter": {
            "min_range": 0,
            "max_range": 10000
        },
        "spatial_filter": {
            "enable": true,
            "hole_filling_radius": 20,
            "alpha": 0.5,
            "delta": 20
        },
        "speckle_filter": {
            "enable": true,
            "speckle_range": 50
        }
    },
    "imu": {
        "ACCELEROMETER_RAW": 500,
        "GYROSCOPE_RAW": 400,
        "ROTATION_VECTOR": 400,
        "batch_threshold": 1,
        "max_batch_reports": 10
    }
}
